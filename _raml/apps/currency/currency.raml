#%RAML 0.8
  displayName: Resource for currency support
  is: [ base ]
  get:
    description: The shop currency information resource
    responses:
      404:
        description: Shop not found
      200:
        body:
          application/json:
            example: |
              {
              "default" : "EUR",
              "items":
                    [
                        "EUR", "USD", "GBP"
                    ]
              }
